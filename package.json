{
  "name": "personal-website",
  "version": "1.0.0",
  "private": true,
  "homepage": "http://Nancy982.github.io/Web-Portfolio",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "gh-pages": "^3.2.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run set-legacy-env && npm run build",
    "deploy": "gh-pages -d build",
    "start:legacy": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "set-legacy-env": "cross-env NODE_OPTIONS=--openssl-legacy-provider"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
